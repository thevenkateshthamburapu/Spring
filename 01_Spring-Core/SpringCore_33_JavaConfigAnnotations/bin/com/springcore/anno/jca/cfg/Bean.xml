<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	
	<context:annotation-config/>
	
	<bean id="car" class="com.springcore.anno.jca.bean.Car">
		<property name="id" value="1001" />
		<property name="name" value="audi" />
	</bean>

	<bean id="eng1" class="com.springcore.anno.jca.bean.Engine">
		<property name="engId" value="2001" />
		<property name="fuelType" value="Petrol" />
	</bean>

	<bean id="eng2" class="com.springcore.anno.jca.bean.Engine">
		<property name="engId" value="2002" />
		<property name="fuelType" value="Diesel" />
	</bean>
	
	<!-- we add context namespace in xml file -->

</beans>
